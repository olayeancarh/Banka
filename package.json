{
  "name": "banka",
  "version": "1.0.0",
  "description": "A simple banking application",
  "main": "index.js",
  "dependencies": {
    "babel-plugin-istanbul": "^5.1.1",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "chai-http": "^4.2.1",
    "cross-env": "^5.2.0",
    "email-validator": "^2.0.4",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.10.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "node ./build/index.js",
    "start:dev": "nodemon --exec babel-node ./src/index.js",
    "test": "nyc --require @babel/register _mocha ./src/api/test/test.js --timeout 15000 --exit",
    "coverage": "cross-env NODE_ENV=test nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/olayeancarh/Banka.git"
  },
  "keywords": [
    "Banking"
  ],
  "author": "olayeancarh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/olayeancarh/Banka/issues"
  },
  "homepage": "https://github.com/olayeancarh/Banka#readme",
  "engines": {
    "node": "10.15.1",
    "npm": "6.4.1"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
